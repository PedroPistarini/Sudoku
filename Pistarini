    coluna_d_facil proc
    
    coluna_d_facil endp

; Altera o valor da posição indicada
    verifica_posicao proc 

        call entra_coluna        

            call coluna_a
    
            call coluna_b

            call coluna_c

            call coluna_d

            call coluna_e

            call coluna_f

            call coluna_g

            call coluna_h

            call coluna_i
           
        ret
    verifica_posicao endp

; Altera o valor da posição indicada 
    verifica_posicao_facil proc

        call entra_coluna

            call coluna_a_facil

            call coluna_b_facil

            jmp teste
            
            call coluna_c_facil

            call coluna_d_facil


    teste:
    ret
    verifica_posicao_facil endp

; Verifica as linhas e colunas com a reposta correta
  ;  verifica_resposta_certa proc

        ;cmp matriz[bx][si], matriz_resposta[bx][si]
       ; jne nao_igual0

           ; mov ah, 09 
            ;lea dx, vencedor
            ;int 21h

        ;nao_igual0:

       ; ret
   ; verifica_resposta_certa endp

; Printa cabeçalho
    cabecalho1 proc

        mov ah, 09
        lea dx, cabecalho
        int 21h

        pula_linha

        mov ah, 09
        lea dx, Titulo
        int 21h

        pula_linha

        mov ah, 09
        lea dx, cabecalho
        int 21h
        
        pula_linha


    ret
    cabecalho1 endp

; Printa regras
    regra proc

        pula_linha

        mov ah, 09
        lea dx, regras1
        int 21h

        pula_linha

        mov ah, 09
        lea dx, regras2
        int 21h

        pula_linha

        mov ah, 09
        lea dx, regras3
        int 21h

        pula_linha
        pula_linha

        mov ah, 09 
        lea dx, cabecalho
        int 21h

        pula_linha
        pula_linha

        mov ah, 09 
        lea dx, press
        int 21h

        mov ah, 01
        int 21h

        pula_linha
        pula_linha

        mov ah, 09 
        lea dx, cabecalho
        int 21h

       
        
        

        limpa_tela

        mov ah, 09 
        lea dx, seleciona_dificuldade
        int 21h

        pula_linha

        mov ah, 09 
        lea dx, dificuldade_facil
        int 21h

        pula_linha

        mov ah, 09 
        lea dx, dificuldade_medio
        int 21h

        pula_linha

        mov ah, 09 
        lea dx, pressione_um_numero
        int 21h

        mov ah, 01
        int 21h

        mov dificuldade, al
        

        call nivel_dificuldade

    ret
    regra endp

; Segmento
    segmento proc

        mov ax, @data
        mov ds, ax

        ret
    segmento endp

; Encerramento
    encerra proc far

        mov ah, 09
        lea dx, encerramento
        int 21h

        mov ah, 01
        int 21h
        
        cmp al, 13
        je encerra1

            pula_linha

            call verifica_posicao
            ;call verifica_resposta_certa

            jmp continua

        encerra1:

        limpa_tela

        call cabecalho1

        pula_linha
        
        call espacao

        mov ah, 09
        lea dx, resposta
        int 21h

        mov ah, 09
        lea dx, letras_colunas
        int 21h

        pula_linha
        pula_linha

        call valida_dificuldade_resposta

        pula_linha
        call espacao
        mov ah, 09
        lea dx, obrigado
        int 21h

        pula_linha

        mov ah, 09 
        lea dx, jogar_novamente
        int 21h

        mov ah, 01
        int 21h

        cmp al, 's'
        jne sim

            jmp comeca_denovo

        sim:
        cmp al, 'S'
        jne sim1

            jmp comeca_denovo

        sim1:

        cmp al, 'n'
        jmp final

        final:
        mov ah, 4ch
        int 21h
    ret
    encerra endp

    valida_dificuldade proc 

        call nivel_dificuldade
        cmp al, 31h
        jne facil1

            call imp_matriz_facil
            jmp continua_programa

        facil1:

        cmp al, 32h
        jne medio1

            call imp_matriz_medio
            jmp continua_programa

        medio1:

        continua_programa:
    ret
    valida_dificuldade endp

    valida_dificuldade_resposta proc

        call nivel_dificuldade
        cmp al, 31h
        jne facil01

            call imp_matriz_resposta_facil
            jmp continua_programa0

        facil01:

        cmp al, 32h
        jne medio01

            call imp_matriz_resposta_medio
            jmp continua_programa0

        medio01:

        continua_programa0:

        ret
    valida_dificuldade_resposta endp

    valida_dificuldade_validacao proc

        call nivel_dificuldade
        cmp al, 31h
        jne facil001

            call verifica_posicao_facil
            jmp continua_programa01

        facil001:

        cmp al, 32h
        jne medio001

            call verifica_posicao
            jmp continua_programa01

        medio001:

        continua_programa01:

    ret
    valida_dificuldade_validacao endp
    
    main proc
    
        comeca_denovo:                      ; Vem da Função encerra para começar denovo o programa

        call segmento

        limpa_tela

        call cabecalho1
        call regra

        limpa_tela

        call cabecalho1

        mov ah, 09
        lea dx, letras_colunas
        int 21h

        pula_linha
        pula_linha

        call valida_dificuldade

        mov cx, 999

        outro_valor:
        
        pula_linha

        call valida_dificuldade_validacao
        
        continua:

        call cabecalho1

        mov ah, 09
        lea dx, letras_colunas
        int 21h

        pula_linha
        pula_linha

        call valida_dificuldade
        call encerra
        
        loop outro_valor 
    
    
    main endp
END main
