    coluna_a_facil proc

        cmp coluna, 'a'
            jne coluna_01
            
                mov bx, 0
                pula_linha

                call entra_linha

                cmp linha, 31h
                jne linha_01

                    mov si, 0

                    call altera_valor
                    limpa_tela
                    jmp fim01
                    
                linha_01:
                
                cmp linha, 32h
                jne linha_02

                    mov bx, 10
                    mov si, 0

                    call altera_valor
                    limpa_tela
                    jmp fim01

                linha_02:

                cmp linha, 33h
                jne linha_03

                    mov bx, 10
                    mov si, 0

                    call altera_valor
                    limpa_tela
                    jmp fim01

                linha_03:

            coluna_01:
            
            cmp coluna, 'a'
            jne coluna_001
                cmp linha, 34h
                jne linha_04

                    mov bx, 15
                    mov si, 0

                    call altera_valor
                    limpa_tela
                    jmp fim01

                linha_04:

            coluna_001:

        fim01:

        ret
    coluna_a_facil endp

    coluna_b_facil proc
    
        cmp coluna, 'b'
            jne coluna_02
            
